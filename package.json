{
	"name": "com.gerhansquad.salah",
	"displayName": "SalahApp",
	"version": "1.0.0",
	"description": "A muslim prayer reminder app",
	"main": "app.js",
	"scripts": {
		"build": "webpack --config webpack.config.js",
		"emulate": "npm run build && cordova emulate android",
		"device": "npm run build && cordova run android --device"
	},
	"keywords": [
		"ecosystem:cordova"
	],
	"author": "Gerhansquad",
	"license": "MIT",
	"devDependencies": {
		"cordova-android": "^9.0.0",
		"cordova-browser": "^6.0.0",
		"cordova-ios": "^6.1.1",
		"cordova-plugin-dialogs": "^2.0.2",
		"cordova-plugin-geolocation": "^4.1.0",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.3.0"
	},
	"cordova": {
		"plugins": {
			"cordova-plugin-geolocation": {
				"GPS_REQUIRED": "true"
			},
			"cordova-plugin-dialogs": {}
		},
		"platforms": [
			"android",
			"ios",
			"browser"
		]
	},
	"dependencies": {
		"cordova": "^11.0.0",
		"cordova-import-npm": "^1.0.33",
		"swiper": "^7.0.1"
	}
}
