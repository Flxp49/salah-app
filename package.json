{
	"name": "com.gerhansquad.salah",
	"displayName": "SalahApp",
	"version": "1.0.0",
	"description": "A muslim prayer assistant app",
	"main": "app.js",
	"scripts": {
		"test": "jest",
		"build": "webpack --config webpack.config.js",
		"emulate": "npm run build && cordova emulate android",
		"device": "npm run build && cordova run android --device"
	},
	"keywords": [
		"ecosystem:cordova"
	],
	"author": "Gerhansquad",
	"license": "MIT",
	"devDependencies": {
		"cordova-android": "^9.0.0",
		"cordova-browser": "^6.0.0",
		"cordova-ios": "^6.1.1",
		"cordova-plugin-add-swift-support": "^2.0.2",
		"cordova-plugin-advanced-http": "^3.1.0",
		"cordova-plugin-file": "^6.0.2",
		"cordova-plugin-geolocation": "^4.1.0",
		"cordova-plugin-nativegeocoder": "^3.4.1",
		"cordova-plugin-splashscreen": "^6.0.0",
		"cordova-plugin-whitelist": "^1.3.4",
		"jest": "^26.6.3",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.3.0"
	},
	"cordova": {
		"plugins": {
			"cordova-plugin-whitelist": {},
			"cordova-plugin-geolocation": {
				"GPS_REQUIRED": "true"
			},
			"cordova-plugin-advanced-http": {},
			"cordova-plugin-nativegeocoder": {
				"LOCATION_WHEN_IN_USE_DESCRIPTION": "Use geocoder service"
			}
		},
		"platforms": [
			"android",
			"ios",
			"browser"
		]
	},
	"dependencies": {
		"cordova-import-npm": "^1.0.33"
	}
}